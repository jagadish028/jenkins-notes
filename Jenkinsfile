                              sleep 5
                              echo "build"
                                   '''
                                   }
                                   }
                                   stage('test') {
                                    steps {
                                          sh '''
                                                sleep 5
                                                      echo "test"
                                                      '''
                                                      }
                                                      }
                                                      
                                                   
                                                 
      }
}
